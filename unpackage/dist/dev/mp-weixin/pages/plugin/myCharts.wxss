.container {
	box-sizing: border-box;
	height: 100%;
	background-color: #efeff4;
}
.content {
	width: 750rpx;
	height: calc(100% - 100rpx);
	background-color: #efeff4;
}
.title {
	line-height: 80rpx;
	background-color: #eee;
	text-indent: 20rpx;
	font-size: 30rpx;
	color: #000000;
	letter-spacing: 1px;
}
.i-cube {
	width: 12rpx;
	height: 60rpx;
	background-color: #007AFF;
	margin-right: 20rpx;
}
.s-btn {
	font-size: 28rpx;
	background-color: transparent;
	color: #f6a121;
	line-height: 1.8;
	padding-left: 20rpx;
	padding-right: 20rpx;
}
.s-btn:after {
	border: 2rpx solid #8799A3;
}
.s-btn-hover {
	background-color: #f6a121;
